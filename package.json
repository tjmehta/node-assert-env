{
  "name": "assert-env",
  "version": "0.4.0",
  "description": "Assert that specific environment variables exist.",
  "main": "index.js",
  "scripts": {
    "test": "tape test.js | faucet",
    "develop": "rerun-script"
  },
  "watches": [
    {
      "script": "test",
      "patterns": [
        "*.js"
      ]
    },
    {
      "script": "lint",
      "patterns": [
        "*.js"
      ]
    }
  ],
  "keywords": [
    "12factor",
    "env",
    "envc",
    "dotenv",
    "assert",
    "assertion",
    "environment",
    "variable",
    "environmentvariables"
  ],
  "author": "Wil Moore III <wil.moore@wilmoore.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wilmoore/node-assert-env"
  },
  "dependencies": {},
  "devDependencies": {
    "chai": "^1.10.0",
    "faucet": "0.0.1",
    "rerun-script": "^0.4.0",
    "tape": "^3.0.1"
  }
}
